{
  "build": {
    "beforeDevCommand": "cls && bun dev",
    "beforeBuildCommand": "cls && bun build",
    "frontendDist": "../dist",
    "devUrl": "http://localhost:1420"
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "resources": [],
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "createUpdaterArtifacts": "v1Compatible"
  },
  "productName": "Axis Launcher",
  "mainBinaryName": "Axis",
  "version": "1.0.0",
  "identifier": "dev.axis",
  "app": {
    "security": {
      "assetProtocol": {
        "scope": [
          "*/**"
        ]
      },
      "csp": null
    },
    "withGlobalTauri": false,
    "windows": [
      {
        "title": "Axis",
        "fullscreen": false,
        "decorations": false,
        "transparent": true,
        "height": 700,
        "width": 1200,
        "devtools": true,
        "resizable": false,
        "maximizable": false,
        "visible": true,
        "label": "main",
        "useHttpsScheme": true,
        "shadow": true
      }
    ]
  }
}
